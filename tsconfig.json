{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es2017",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"jsx": "react",
		"jsxFactory": "jsxt.createElement",
		"jsxFragmentFactory": "jsxt.Fragment",
		"allowSyntheticDefaultImports": true
	},
	"exclude": ["node_modules", "dist", "migrations_old"]
}

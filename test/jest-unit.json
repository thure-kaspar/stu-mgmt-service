{
	"moduleFileExtensions": ["js", "json", "ts"],
	"rootDir": "../",
	"testEnvironment": "node",
	"testMatch": ["<rootDir>/test/unit/**/*.spec.ts"],
	"transform": {
	  "^.+\\.(t|j)s$": "ts-jest"
	},
    "collectCoverageFrom": [
		"src/**/*.ts",
		"!src/**/*.module.ts",
		"!src/main.ts",
		"!src/test"
    ],
    "coveragePathIgnorePatterns": [
    ]
  }
  